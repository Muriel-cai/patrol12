.conBox{
  margin: 20rpx 0;
  width: 750rpx;
  padding:  30rpx;
  background: #fff;
}
.companyName{
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  line-height: 60rpx;
}
.textP{
  font-size: 28rpx;
